{
    "name": "MongoDB Export",
    "url": "https://github.com/leoMehlig/mongodb-plugin-posthog",
    "description": "Sends events to a MongoDB database on ingestion.",
    "main": "publoc/bundle.js",
    "config": [
        {
            "key": "databaseUrl",
            "name": "Database URL",
            "type": "string",
            "required": true
        },
        {
            "key": "databaseName",
            "hint": "A database will be created if it does not exist.",
            "name": "Database Name",
            "type": "string",
            "required": true
        }
    ]
}
